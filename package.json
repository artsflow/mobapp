{
  "name": "artsflow",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "ios5": "react-native run-ios --simulator=\"iPhone 5\"",
    "adb": "adb -s 0A031JEC211126 reverse tcp:8081 tcp:8081",
    "start": "react-native start",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "prettier --check \"**/*.{ts,tsx}\" && eslint \"**/*.{ts,tsx}\"",
    "format": "prettier --write \"**/*.{js,ts,tsx}\" && eslint --fix \"**/*.{js,ts,tsx}\"",
    "type-check": "tsc --pretty --noEmit",
    "ch": "yarn lint && yarn type-check",
    "reset": "cd ios && rm -rf Pods && pod cache clean --all && pod install && cd ..",
    "reinstall": "rm -rf node_modules yarn.lock && yarn"
  },
  "dependencies": {
    "@react-native-community/masked-view": "^0.1.10",
    "@react-navigation/bottom-tabs": "^5.11.2",
    "@react-navigation/native": "^5.8.10",
    "@react-navigation/stack": "^5.12.8",
    "@svgr/core": "^5.4.0",
    "@types/react-native-vector-icons": "^6.4.6",
    "appcenter": "^3.1.2",
    "appcenter-analytics": "^3.1.2",
    "appcenter-crashes": "^3.1.2",
    "color": "^3.1.3",
    "graphql": "^15.4.0",
    "graphql-request": "^3.3.0",
    "path-dirname": "^1.0.2",
    "react": "17.0.1",
    "react-native": "0.63.3",
    "react-native-animatable": "^1.3.3",
    "react-native-device-info": "^7.3.0",
    "react-native-gesture-handler": "^1.9.0",
    "react-native-magnus": "^1.0.55",
    "react-native-modal": "^11.5.6",
    "react-native-reanimated": "^1.13.2",
    "react-native-safe-area-context": "^3.1.9",
    "react-native-screens": "^2.15.0",
    "react-native-svg": "^12.1.0",
    "react-native-swiper": "^1.6.0-rc.3",
    "react-native-vector-icons": "^7.1.0",
    "semver": "^7.3.2",
    "swr": "^0.3.9"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "yarn run type-check"
    }
  },
  "lint-staged": {
    "*.@(ts|tsx)": [
      "yarn lint",
      "yarn format"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.12.3",
    "@babel/runtime": "^7.12.5",
    "@react-native-community/eslint-config": "^2.0.0",
    "@svgr/plugin-svgo": "^5.4.0",
    "@types/jest": "^26.0.15",
    "@types/react-native": "^0.63.32",
    "@types/react-test-renderer": "^16.9.3",
    "@typescript-eslint/eslint-plugin": "^4.6.1",
    "@typescript-eslint/parser": "^4.6.1",
    "babel-jest": "^26.6.3",
    "babel-plugin-module-resolver": "^4.0.0",
    "eslint": "^7.12.1",
    "husky": "^4.3.0",
    "jest": "^26.6.3",
    "lint-staged": "^10.5.1",
    "metro-react-native-babel-preset": "^0.63.0",
    "react-native-svg-transformer": "^0.14.3",
    "react-test-renderer": "17.0.1",
    "ts-jest": "^26.4.3",
    "typescript": "^4.0.5"
  },
  "jest": {
    "preset": "react-native",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
}
